{
  "private": true,
  "workspaces": [
    "./packages/common/*",
    "./packages/web"
  ],
  "name": "time-back",
  "version": "1.0.0",
  "description": "Time back. Lik Pomodoro but with strawberries.",
  "author": "bogdan petru pintican <bogdan.pintican@8x8.com>",
  "license": "MIT",
  "scripts": {
    "reset": "find . -type d -name 'node_modules' -exec rm -r {} +  && yarn",
    "start": "yarn workspace @app/web start",
    "build": "yarn workspace @app/web build",
    "deploy": "yarn workspace @app/web deploy"
  }
}
